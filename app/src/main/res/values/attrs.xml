<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FloatingTitleEditText">
        <attr name="hint" format="string|reference" />
        <attr name="android:drawableEnd" />
        <attr name="title" format="string|reference" />
        <attr name="minimumHeight" format="dimension|reference" />
        <attr name="maxElements" format="integer" />
        <attr name="commentField" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SeparatorView">
        <attr name="color" />
        <attr name="alpha" />
    </declare-styleable>

    <declare-styleable name="DriverCardView">
        <attr name="image" format="string" />
        <attr name="name" format="string" />
    </declare-styleable>
</resources>